@model TrainingScene
@{ Layout = "~/Views/Shared/_Layout.cshtml";
 ViewBag.Title = "Get TrainingScene by Id";}
    <link rel="stylesheet" type="text/css" href="~/css/site.css">
<h2>Get TrainingScene by ID <a asp-action="Index" class="btn btn-sm btn-primary">Back</a></h2>
<form method="post">
    <div class="form-group">
        <label for="id">ID:</label>
        <input class="form-control" name="id" />
    </div>
    <div class="text-center panel-body">
        <button type="submit" class="btn btn-sm btn-primary">Get TrainingScene</button>
    </div>
</form>

@if (Model != null)
{
    <h2>TrainingScene</h2>
    <table class="table table-sm table-striped table-bordered m-2">
        <thead><tr><th>ID</th><th>Name</th><th>Description</th><th>Patient ID</th><th>Scenario ID</th></tr></thead>
        <tbody>
            <tr>
                <td>@Model.TrainingSceneID</td>
                <td>@Model.TrainingSceneName</td>
                <td>@Model.TrainingSceneDescription</td>
                <td>@Model.PatientID</td>
                <td>@Model.TrainingScenarioID</td>
            </tr>
        </tbody>
    </table>
}